{
  "expo": {
    "name": "CareAI",
    "slug": "careai",
    "owner": "jacomostert",
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/splash-static.png",
    "backgroundColor": "#FAFAFA",
    "splash": {
      "image": "./assets/images/splash-static.png",
      "resizeMode": "contain",
      "backgroundColor": "#FAFAFA"
    },
    "ios": {
      "supportsTablet": true
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS",
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS"
      ]
    },
    "web": {
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-audio"
    ],
    "extra": {
      "eas": {
        "projectId": "88888138-33b6-4b11-ba97-50ab0494d2b8"
      }
    },
    "updates": {
      "url": "https://u.expo.dev/88888138-33b6-4b11-ba97-50ab0494d2b8"
    }
  },
  "updates": {
    "url": "https://u.expo.dev/88888138-33b6-4b11-ba97-50ab0494d2b8"
  }
}
